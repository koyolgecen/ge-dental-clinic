{
  "root": ".",
  "https_only": true,
  "clean_urls": false,
  "gzip": true,
  "brotli": true,
  "headers": {
    "/**/*.html": {
      "Cache-Control": "no-cache, no-store, must-revalidate"
    },
    "/**/*.{css,js}": {
      "Cache-Control": "public, max-age=31536000, immutable"
    },
    "/**/*.{webp,avif,jpg,jpeg,png,svg,gif,ico}": {
      "Cache-Control": "public, max-age=31536000, immutable"
    }
  },
  "error_page": "404.html"
}